<!-- PAGE 14: N COLOR MATCHING -->
<div class="page" id="14" style="display: none;">
<div class="cell">

<div align="right">
    <input type="button" value="Continue" onclick="function filledOut(id) { return document.getElementById(id).value != 'default' } if(filledOut('lightDressvRoom_Y')) { myNext(); hidePicker1();showPicker2();}else{ alert('Please answer all of the questions marked with an asterisk (*).'); }" />
</div>

<div align="center" >
    <img id="dress1" src="http://khermann.scripts.mit.edu/Scale/dress_100_A.jpg" alt="Dress1" style="width:226px;height:337px">
</div>

    <!--invisible textbox to store the picked color-->
    <input id="pickedColorA_N" type="text" name="pickedColorNameA_N" style="visibility:hidden">

    <script type="text/javascript">
        //document.getElementById("dress2").onload = function() {alert("thing")};

        function showPicker1(arg){
            var mainExpImHeight = 436
                mainExpImWidth  = 402

                imWidth  = $("#dress1").width(),
                imHeight = $("#dress1").height(),
                imTop    = $("#dress1").position().top,
                imRight  = $("#dress1").position().left + $("#dress1").width(),
                cpDiam   = (5/10) * mainExpImHeight,
                buffer   = mainExpImWidth / 7,

                swatchSide = cpDiam/4,
                swatchTop  = imTop + cpDiam - swatchSide - 8, //the 8 corrects for the border
                swatchLeft = imRight + buffer + cpDiam + buffer/4,
                eps        = buffer/10

            cp = Raphael.colorpicker(imRight + buffer, imTop, (5/10) * mainExpImHeight, "#eee");

            //should just div, not make paper
            var swatch = document.createElement("div")
                swatch.id                 = "swatch"
                swatch.style.position     = "absolute";
                swatch.style.left         = swatchLeft  + 'px';
                swatch.style.top          = swatchTop  + 'px';
                swatch.style.width        = swatchSide + 'px';
                swatch.style.height       = swatchSide + 'px';
                swatch.style.background   = cp.color()
                swatch.style.border       = "4px solid white"
                document.body.appendChild(swatch)

            cp.onchange = function(){
                swatch.style.background = cp.color()
            }
        }
        function hidePicker1(){
            //store the picked value, which is cp.color()
            document.getElementById("pickedColorA_N").value = cp.color()
            cp.remove()
            $('#swatch').remove()
        }
    </script>

<p><b>Now we would like you to report on the color of the pixels you see in the image.  We will provide a color-matching tool.  Please adjust the hue (color circle) and brightness (slider bar) to match the pixels you see in the image.</b></p>

<p><b>*1. What color is the dress where the arrow is pointing (neckline)?</b></p>

<br />
<br />

</div>
</div>

