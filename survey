<script type="text/javascript">
    function newSubj() {
       // screen_size
       var ss = {
           screen_width: screen.width,
           screen_height: screen.height,
           screen_depth: screen.pixelDepth
       };
       // browser_size
       var bs = {
           browser_width: window.innerWidth || document.body.clientWidth,
           browser_height: window.innerHeight || document.body.clientHeight
       };
       // plugins
       var plugins = [];
       $.each(navigator.plugins, function() {
           plugins.push(this.name);
       });
       var os = navigator.platform;

       document.getElementById("os").value = os
       document.getElementById("bsWidth").value = bs.browser_width
       document.getElementById("bsHeight").value = bs.browser_height
       document.getElementById("ssWidth").value = screen.width
       document.getElementById("ssHeight").value =screen.height
       document.getElementById("ssPixdepth").value =screen.pixelDepth
       document.getElementById("plugins").value = plugins

       }
</script>

