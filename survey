<!-- bnt YELLOW: Colorpicker A -->
<div class="page" id="48" style="display: none;">
<div class="cell">

<div align="right">
    <input type="button" value="Continue" onclick="function filledOut(id) { return document.getElementById(id).value != 'default' } if(filledOut('lightDressvRoom_Y')) { myNext(); hidePicker23(); showPicker24()}else{ alert('Please answer all of the questions marked with an asterisk (*).'); }" />
</div>

<div align="center" >
    <img id="dress23" src="http://khermann.scripts.mit.edu/Scale/bnt_yellow_A.jpg" alt="Dress1" style="width:518px;height:429px">
</div>

    <!--invisible textbox to store the picked color-->
    <input id="pickedColor_bnt_yellow_A" type="text" name="pickedColorName_bnt_yellow_A" style="visibility:hidden">

    <script type="text/javascript">
        //document.getElementById("dress2").onload = function() {alert("thing")};

        function showPicker23(arg){
            var mainExpImHeight = 436
                mainExpImWidth  = 402

                imWidth  = $("#dress23").width(),
                imHeight = $("#dress23").height(),
                imTop    = $("#dress23").position().top,
                imRight  = $("#dress23").position().left + $("#dress23").width(),
                cpDiam   = (5/10) * mainExpImHeight,
                buffer   = mainExpImWidth / 7,

                swatchSide = cpDiam/4,
                swatchTop  = imTop + cpDiam - swatchSide - 8, //the 8 corrects for the border
                swatchLeft = imRight + buffer + cpDiam + buffer/4,
                eps        = buffer/10

            cp = Raphael.colorpicker(imRight + buffer, imTop, (5/10) * mainExpImHeight, "#eee");

            //should just div, not make paper
            var swatch = document.createElement("div")
                swatch.id                 = "swatch"
                swatch.style.position     = "absolute";
                swatch.style.left         = swatchLeft  + 'px';
                swatch.style.top          = swatchTop  + 'px';
                swatch.style.width        = swatchSide + 'px';
                swatch.style.height       = swatchSide + 'px';
                swatch.style.background   = cp.color()
                swatch.style.border       = "4px solid white"
                document.body.appendChild(swatch);

            cp.onchange = function(){
                swatch.style.background = cp.color()
            }
        }
        function hidePicker23(){
            //store the picked value, which is cp.color()
            document.getElementById("pickedColor_bnt_yellow_A").value = cp.color()
            cp.remove()
            $('#swatch').remove()
        }
    </script>

<p><b>Please adjust the hue (color circle) and brightness (slider bar) to match the pixels you see in the image.</b></p>

<p><b>*2. What color is the dress at (A)?</b></p>

<br />
<br />

</div>
</div>

