    <script type="text/javascript">
        //document.getElementById("dress2").onload = function() {alert("thing")};

        function showPicker17(arg){
            var mainExpImHeight = 436
                mainExpImWidth  = 402

                imWidth  = $("#dress17").width(),
                imHeight = $("#dress17").height(),
                imTop    = $("#dress17").position().top,
                imRight  = $("#dress17").position().left + $("#dress17").width(),
                cpDiam   = (5/10) * mainExpImHeight,
                buffer   = mainExpImWidth / 7,

                swatchSide = cpDiam/4,
                swatchTop  = imTop + cpDiam - swatchSide - 8, //the 8 corrects for the border
                swatchLeft = imRight + buffer + cpDiam + buffer/4,
                eps        = buffer/10

            cp = Raphael.colorpicker(imRight + buffer, imTop, (5/10) * mainExpImHeight, "#eee");

            //should just div, not make paper
            var swatch = document.createElement("div")
                swatch.id                 = "swatch"
                swatch.style.position     = "absolute";
                swatch.style.left         = swatchLeft  + 'px';
                swatch.style.top          = swatchTop  + 'px';
                swatch.style.width        = swatchSide + 'px';
                swatch.style.height       = swatchSide + 'px';
                swatch.style.background   = cp.color()
                swatch.style.border       = "4px solid white"
                document.body.appendChild(swatch);

            cp.onchange = function(){
                swatch.style.background = cp.color()
            }
        }
        function hidePicker17(){
            //store the picked value, which is cp.color()
            document.getElementById("pickedColor_solid_blue_A").value = cp.color()
